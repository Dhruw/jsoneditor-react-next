{
  "name": "jsoneditor-react-next",
  "version": "1.0.1",
  "description": "React implementation of https://github.com/josdejong/jsoneditor",
  "main": "es",
  "module": "es",
  "files": [
    "es",
    "*.md",
    "docs"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env BABEL_ENV=test NODE_ENV=test start-storybook -p 9001 -c .storybook",
    "lint": "eslint src stories --ext .js --ext .jsx --fix --config .eslintrc.json",
    "build:es": "cross-env BABEL_ENV=production NODE_ENV=production rollup -c rollup.es.config.js -i src/index.js -o es/index.js",
    "build": "npm run build:es && uglifycss es/editor.css > es/editor.min.css",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsukhu/jsoneditor-react-next.git"
  },
  "keywords": [
    "react",
    "jsoneditor",
    "json"
  ],
  "author": "Ivan Kopeykin, Tarun Kumar Sukhu, Divyanshu Dhruw",
  "license": "MIT",
  "bugs": {
    "url": "git+https://github.com/tsukhu/jsoneditor-react-next/issues"
  },
  "homepage": "https://github.com/tsukhu/jsoneditor-react-next#readme",
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "ajv": "^8.12.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "brace": "^0.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsoneditor": "^9.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-form": "^8.3.10",
    "rollup": "^3.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^4.3.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "uglifycss": "=0.0.29"
  },
  "peerDependencies": {
    "jsoneditor": "^9.0.0",
    "react": "^18.2.0"
  },
  "browserslist": [
    "defaults"
  ]
}
